{
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "git",
            "url": "git@github.com:highwire/hwphp.git"
        },
        {
            "type": "git",
            "url": "git@github.com:highwire/freebird.git"
        },
        {
            "type": "git",
            "url": "git@github.com:phayes/elasticentityquery.git"
        }
    ],
    "require": {
        "highwire/freebird": "dev-8.x-1.x-dev",
        "oomphinc/composer-installers-extender": "^1.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/SiteScriptHandler.php"
        ]
    },
    "scripts": {
        "required-files": "HighWireSite\\composer\\SiteScriptHandler::createSiteSymlinks",
        "site-symlinks": "HighWireSite\\composer\\SiteScriptHandler::createSiteSymlinks",
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "build-themes": "DrupalComposer\\DrupalScaffold\\Plugin::buildThemes",
        "post-install-cmd": [
            "DrupalComposer\\DrupalScaffold\\Plugin::buildThemes",
            "HighWireSite\\composer\\SiteScriptHandler::createRequiredFiles",
            "HighWireSite\\composer\\SiteScriptHandler::createSiteSymlinks",
            "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
        ],
        "post-update-cmd": [
            "HighWireSite\\composer\\SiteScriptHandler::createRequiredFiles",
            "HighWireSite\\composer\\SiteScriptHandler::createSiteSymlinks"
        ]
    },
    "extra": {
        "installer-types": [
            "project"
        ],
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        }
    }
}
